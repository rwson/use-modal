{
  "name": "@usemodals/vue",
  "version": "1.0.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "modal",
    "vue modal",
    "open modal",
    "close modal",
    "update modal",
    "close all modals"
  ],
  "repository": {
    "url": "https://github.com/rwson/use-modal",
    "type": "git"
  },
  "bugs": "https://github.com/rwson/use-modal/issues",
  "homepage": "https://github.com/rwson/use-modal#readme",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "link:local": "yarn build && yalc publish --push"
  },
  "main": "./dist/use-modals-vue.cjs.js",
  "module": "./dist/use-modals-vue.es.js",
  "typings": "./dist/index.d.ts",
