{
  "name": "@use-modals/vue3",
  "version": "1.0.1",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "link:local": "yarn build && yalc publish --push"
  },
  "main": "./dist/use-modals-vue3.cjs.js",
  "module": "./dist/use-modals-vue3.es.js",
  "typings": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/use-modals-vue3.es.js",
      "require": "./dist/use-modals-vue3.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.4.2"
  }
}
